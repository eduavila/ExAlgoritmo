algoritmo "exe_28"


var
   a,b,soma,prod, quoci :real
inicio
      escreva("Forneça Dois numero real :! ")
      leia(a)
      leia(b)
      soma <- a + b
      prod <- a*b
      quoci <- a / b
      escreval("A soma  dos valores : ", soma)
      escreval("O produto dos valores : ", prod)
      escreval("Quociente dos valores : ", quoci)
fimalgoritmo
